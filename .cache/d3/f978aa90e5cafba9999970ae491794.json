er: https://material.io/design/layout/spacing-methods.html#touch-targets */\r\n  position: absolute;\r\n  transform: translateX(90px);\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\nnav {\r\n  min-height: 72px;\r\n}\r\n\r\nnav ul {\r\n  text-align: center;\r\n}\r\n\r\nnav li {\r\n  padding: 15px\r\n}\r\n\r\n#blogDiv {\r\n  font-family: 'Questrial', sans-serif;\r\n}\r\n\r\n.entryDiv {\r\n  border: 2px solid pink;\r\n  margin: 20px;\r\n  font-size: 12px;\r\n}\r\n\r\n.entryDetails {\r\n  font-size: 10px;\r\n}\r\n\r\n\r\nfooter {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding: 1em;\r\n  font-size: 8px;\r\n}\r\n\r\n/* hide nav links, class will be removed later with JS */\r\n.is-hidden--mobile {\r\n  font-size: 0;\r\n  height: 0;\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n\r\n  nav ul {\r\n      display: flex;\r\n      justify-content: space-around;\r\n    }\r\n\r\n  .is-hidden--tablet {\r\n      font-size: 0;\r\n      height: 0;\r\n      display: none;\r\n  }\r\n\r\n  .is-shown--tablet {\r\n      height: 100%;\r\n      font-size: initial;\r\n  }\r\n\r\n  section p {\r\n      max-width: 600px;\r\n    }\r\n\r\n}\r\n\r\n@media screen and (min-width: 1024px) {\r\n  header { \r\n      display: flex;\r\n      flex-direction: row;\r\n      margin-right: 1em;\r\n      padding: .5em;\r\n  }\r\n\r\n  .hero {\r\n      height: 40vh;\r\n  }\r\n\r\n  section,\r\n  .section-container {\r\n      display: flex; \r\n  }\r\n\r\n  section {\r\n      \r\n      flex-direction: column;\r\n  }\r\n\r\n  .section--btn {\r\n      max-width: 150px;\r\n      text-align: center;\r\n  }\r\n\r\n  section p:last-of-type {\r\n      margin-bottom: auto;\r\n  }\r\n\r\n}"},"lineCount":170}},"error":null,"hash":"6a2769b3263d8aaff1a9d01edf5af260","cacheData":{"env":{}}}